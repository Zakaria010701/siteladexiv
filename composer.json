{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "amidesfahani/filament-tinyeditor": "^1.1",
        "barryvdh/laravel-dompdf": "^3.0",
        "bezhansalleh/filament-panel-switch": "^2.0",
        "bezhansalleh/filament-shield": "^4.0",
        "cmixin/business-day": "^1.19",
        "ezyang/htmlpurifier": "*",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "filament/spatie-laravel-settings-plugin": "^4.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel-notification-channels/sms77": "^1.0",
        "laravel/framework": "^12.0",
        "laravel/pulse": "^1.2",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.8",
        "league/html-to-markdown": "^5.1",
        "livewire/livewire": "^3.4",
        "livewire/volt": "^1.0",
        "nemiah/php-fints": "^3.6",
        "plesk/ext-laravel-integration": "^7.0",
        "predis/predis": "^2.2",
        "pxlrbt/filament-spotlight": "^2.0",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-google-calendar": "^3.8",
        "spatie/laravel-medialibrary": "^11.15",
        "ysfkaya/filament-phone-input": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0.0",
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "^4.0",
        "larastan/larastan": "^3.0",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.14",
        "laravel/sail": "^1.28",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "spatie/laravel-ignition": "^2.0",
        "vcian/laravel-db-auditor": "^1.6"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan filament:assets"
        ],
        "post-install-cmd": [
            "@php artisan filament:assets"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
